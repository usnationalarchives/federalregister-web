<div class="title document-issue">
  <span>Stubbed <%= @presenter.date %> Issue</span>
</div>

<!-- BOOTSTRAP SCOPE START -->
<div class="bootstrap-scope">
<!-- BOOTSTRAP SCOPE START -->

  <div class="wrapper">
    <div class="container fixed">

      <div class="row">
        <div class="col-xs-12 col-md-12">
          <%= render partial: 'issues/document_summary',
              locals: {presenter: @doc_presenter} %>
        </div>
      </div>

      <div class="row">
        <div class="col-xs-9 col-md-9 doc-content doc-toc">
        </div>
        <div class="col-xs-3 col-md-3">
          <%= render partial: 'document_issues/subscribe' %>
        </div>
      </div>

      <div class="row">
        <div class="col-xs-9 col-md-9 doc-content doc-toc">
          <div class="row doc doc-document-issue">
            <%= render partial: 'document_issues/table_of_contents', locals: {
              agencies: @presenter.agencies,
              document_partial: @presenter.document_partial
            } %>
          </div>
        </div>
        <div class="col-xs-3 col-md-3">
          <%= render partial: 'document_issues/agency_count_bar', locals: {
                date: @presenter.date,
                agencies: @presenter.agencies
              } %>
        </div>
      </div>

      <div class="clearfix" style="height: 100px;"></div>
    </div>
  </div>

<!-- BOOTSTRAP SCOPE END -->
</div>
<!-- BOOTSTRAP SCOPE END -->
